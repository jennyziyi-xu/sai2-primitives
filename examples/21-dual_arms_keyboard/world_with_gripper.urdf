<?xml version="1.0" ?>

<world name="demo_world" gravity="0.0 0.0 -9.81">

	<robot name="PANDA">
		<model dir="${EXAMPLE_21_FOLDER}/panda" path="panda_arm_with_gripper.urdf" name="panda_arm_with_gripper" />
		<!-- <model dir="${EXAMPLE_21_FOLDER}/panda" path="panda_arm_hand.urdf" name="panda_arm_hand" /> -->
		<origin xyz="0.0 -0.3 0.0" rpy="0 0 0" />
	</robot>

	<robot name="PANDA2">
		<model dir="${EXAMPLE_21_FOLDER}/panda" path="panda_arm_with_gripper.urdf" name="panda_arm_with_gripper" />
		<!-- <model dir="${EXAMPLE_21_FOLDER}/panda" path="panda_arm_hand.urdf" name="panda_arm_hand" /> -->
		<origin xyz="0.0 0.3 0.0" rpy="0 0 0" />
	</robot>

	<static_object name="Floor">
		<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
		<collision>
			<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
			<geometry>
				<box size="3.0 3.0 0.1" />
			</geometry>
		</collision>
		<visual>
			<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
			<geometry>
				<box size="1.0 3.0 0.06" />
			</geometry>
			<material name="material_gray">
				<color rgba="0.5 0.3 0.3 1.0" />
			</material>
		</visual>
	</static_object>

	<dynamic_object name="apple">
		<origin xyz="0.5 0.27 0.2" rpy="0.0 0.0 0.0" />

		<inertial>
			<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
			<mass value="1" />
			<inertia ixx="0.1" iyy="0.1" izz="0.1" ixy="0" ixz="0" iyz="0" />
		</inertial>

		<visual>
			<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
			<geometry>
				<mesh filename="${EXAMPLE_21_FOLDER}/apple/shape/visual/apple-agveuv-base_link.obj" scale="0.8 0.8 0.8" />
			</geometry>
		</visual>
		<collision>
			<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
			<geometry>
				<box size="0.06 0.06 0.06" />
			</geometry>
		</collision>
	</dynamic_object>

	<static_object name="countertop">
		<origin xyz="0.5 0.0 0.1" rpy="0 0 0" />

		<visual>
			<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
			<geometry>
				<mesh filename="${EXAMPLE_21_FOLDER}/countertop/shape/visual/countertop-epaijd-base_link.obj" scale="1.3 1.0 1.0" />
			</geometry>
		</visual>

		<collision>
			<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
			<geometry>
				<box size="1.0 3.0 0.06" />
			</geometry>
		</collision>

	</static_object>



